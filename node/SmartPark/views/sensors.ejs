<!DOCTYPE html>
<html>

<head>
  <% include head.ejs %>
  
</head>

<body>
  <header>
    <% include header.ejs %>
  </header>
  <main>
    
    
    <div class="container">
      <div class="row">
        <div class="col-md-12">
          <div >
            <form id='timeForm' action = "" method = "GET">
                <input type="hidden" name="sensor" value=<%= result[0].sensor %>>
                Date and Time : 
                  <input type="datetime-local" id="daytime">
                  
                  <input type = "submit" value = "Submit" id="daytime_submit">
            </form>
          </div>
        </div> 
      </div>
    </div>
          
      

      
      <% for(var i=0;i<result.length;i++) {%>
        

        <div class="panel panel-default panel-table">
          <div class="panel-heading">
            <div class="row">
              <% if(result[i]!=null && result[i].hasOwnProperty('keys')) {  %>
                  <p class="panel-title"><%= result[i].sensor %> of Team <%= result[i].teamID %></p>
              <% } else { %>
                   <h3 class="panel-title">Error Showing Data</h3>
                <% } %>
            </div>
            <button type="button" class="btn btn-info fullwidth graphBtn" >Open Graph</button>
            <div class="row graph collapse in">
              <canvas id="line<%= i %>" class="line"></canvas>
            </div>
          </div>

          <div class="panel-body">
            <% for(var i=0;i<result.length;i++) {%>
              <% var tableData = result[i]; %>
              <% include sensortable.ejs %>
              <br><br>
            <% } %>
          </div>
        <br><br>
      <% } %>
   
    </div>
  </main>
  <footer>
    <% include footer.ejs %>
  </footer>
  <script>var frontResult = <%- JSON.stringify(result) %>; </script>
  <script src="/js/moment.js"></script>
  <script src="/js/Chart.min.js"></script>
  <script src="/js/script.js"></script>
</body>

</html>
